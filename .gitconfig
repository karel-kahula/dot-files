[rerere]
    enabled = 1

[alias]
    br = branch
    ci = commit -v
    cp = cherry-pick
    st = status
    co = checkout
    amend = commit --ammend -v
    ls = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate
    ah = "!git config -l | grep alias | cut -c 7-"
    gr = grep -i
[core]
    excludesfile = /home/kkahula/.gitignore
    editor = vim
